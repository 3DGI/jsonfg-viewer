{
  "name": "ninja",
  "version": "0.3.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "parcel build public/index.html public/*.svg --out-dir ./dist/ --public-url . --no-cache",
    "lint": "vue-cli-service lint",
    "electron:build": "vue-cli-service electron:build",
    "electron:serve": "vue-cli-service electron:serve",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "dev": "concurrently \"parcel watch public/index.html public/*.svg --out-dir ./dist/ --public-url . --no-cache\" \"static-server\""
  },
  "main": "background.js",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.13.0",
    "bootstrap": "^4.4.1",
    "bootstrap-vue": "^2.11.0",
    "cityjson-vue-components": "git+https://github.com/cityjson/cityjson-vue-components.git#threejs-loader",
    "core-js": "^3.21.1",
    "earcut": "^2.2.2",
    "jquery": "^3.4.1",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "three": "^0.127.0",
    "vue": "^2.6.11",
    "vue-hot-reload-api": "^2.3.4",
    "vue2-filters": "^0.12.0"
  },
  "devDependencies": {
    "@vue/component-compiler-utils": "^3.3.0",
    "autoprefixer": "^10.4.4",
    "babel-eslint": "^10.1.0",
    "concurrently": "^6.2.1",
    "electron": "^12.0.2",
    "eslint": "^7.24.0",
    "eslint-config-mdcs": "^5.0.0",
    "eslint-plugin-vue": "^7.9.0",
    "parcel-bundler": "^1.12.5",
    "postcss": "^8.1.0",
    "static-server": "^2.2.1",
    "vue-cli-plugin-electron-builder": "^2.0.0-rc.6",
    "vue-template-compiler": "^2.6.11"
  }
}
